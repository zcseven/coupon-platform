// api
syntax = "v1"
info(
    author: "seven"
    date:   "2023-04-10"
    desc:   "user信息"
)

type(
    UserReq{
        Uid int64 `json:"uid"`
        Telephone string `json:"Telephone,optional"`
    }

    UserResp{
        Uid int64 `json:"uid"`
        Username string `json:"user_name"`
    }
)

// service block
@server(
    //jwt:   Auth
    group: user
)
service user-api{
    @doc "user"
    @handler GetUserInfo
    get /info (UserReq) returns (UserResp)
}